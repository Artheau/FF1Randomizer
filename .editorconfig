root = true

[*]
insert_final_newline = true
trim_trailing_whitespace = true

[*.{cs,xaml}]
indent_size = 4
indent_style = tab
end_of_line = crlf

[*.{config,csproj,yml,yaml}]
indent_size = 2
indent_style = space

[*.{js,cshtml}]
indent_size = 4
indent_style = tab
end_of_line = lf
